          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">def allUsers = props.get(&quot;all_users&quot;) ?: []
def content = &quot;username,password\n&quot;
allUsers.each { user -&gt;
    content += user + &quot;\n&quot;
}

def filePath = &quot;D:/测试资料/性能监控环境搭建/学习课件/test_data.csv&quot;

try {
    new File(filePath).parentFile.mkdirs()
    new File(filePath).write(content, &quot;UTF-8&quot;)
    log.info(&quot;✅ 成功生成账号文件: &quot; + filePath)
} catch (Exception e) {
    log.error(&quot;❌ 写入文件失败: &quot; + e.getMessage(), e)
}</stringProp>
