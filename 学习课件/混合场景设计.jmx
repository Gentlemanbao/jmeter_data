          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 预处理程序" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// 定义前缀列表（如 [&quot;user&quot;, &quot;admin&quot;, &quot;test&quot;]）
def prefixList = [&quot;user&quot;, &quot;admin&quot;, &quot;test&quot;]
// 随机选择前缀
def prefix = prefixList[new Random().nextInt(prefixList.size())]
// 定义随机数范围（如 100~999，生成三位随机数）
def randomNum = new Random().nextInt(900) + 100  // 生成 100~999 的随机数
// 拼接用户名（如 &quot;user123&quot;、&quot;admin456&quot;）
def username = prefix + randomNum
// 存储到 JMeter 变量（vars.put）
vars.put(&quot;user_name&quot;, username)

// 生成随机密码
def password = new Random().nextInt(999999) + 111111
vars.put(&quot;user_password&quot;, password.toString()) 

// 添加到全局列表（关键！）
def allUsers = props.get(&quot;all_users&quot;) ?: []
allUsers.add(&quot;${username},${password}&quot;)
props.put(&quot;all_users&quot;, allUsers)

log.info(&quot;✅ 已生成用户：${username},${password}&quot;)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="TestPlan.comments">用这个来生成用户名和密码，注册所需要的参数值</stringProp>
